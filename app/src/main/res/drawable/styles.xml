<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="tvSmall">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="tvHeading">
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="tvHomeButtonText">
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textSize">18sp</item>

    </style>

    <style name="btSolid">
        <item name="android:background">@drawable/shape_solid</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white2</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="CustomDatePickerStyle" parent="Widget.MaterialComponents.MaterialCalendar">
        <item name="android:background">@color/white</item>
    </style>


    <style name="btOutline">
        <item name="android:background">@drawable/shape_outline</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/c10</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="tvNumber" parent="tvHeading">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textSize">40sp</item>
    </style>



    <style name="etTextStyle">
        <item name="android:background">@drawable/shape_outline_gray</item>
        <item name="android:ems">10</item>
        <item name="android:inputType">text</item>
        <item name="android:paddingHorizontal">16dp</item>
    </style>
</resources>